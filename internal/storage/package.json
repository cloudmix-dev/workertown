{
  "name": "@workertown/internal-storage",
  "description": "Internal storage implementations for WorkerTown",
  "version": "0.0.2-alpha.14",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./d1": {
      "default": "./dist/d1.js",
      "types": "./dist/d1.d.ts"
    },
    "./planetscale": {
      "default": "./dist/planetscale.js",
      "types": "./dist/planetscale.d.ts"
    },
    "./sqlite": {
      "default": "./dist/sqlite.js",
      "types": "./dist/sqlite.d.ts"
    },
    "./turso": {
      "default": "./dist/turso.js",
      "types": "./dist/turso.d.ts"
    },
    "./memory": {
      "default": "./dist/memory.js",
      "types": "./dist/memory.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@libsql/kysely-libsql": "~0.3.0",
    "kysely": "~0.26.1",
    "kysely-d1": "~0.3.0",
    "kysely-planetscale": "~1.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "~4.20230710.1",
    "@planetscale/database": "~1.10.0",
    "better-sqlite3": "~8.5.0"
  },
  "peerDependencies": {
    "@planetscale/database": "~1.10.0",
    "better-sqlite3": "~8.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
